<h1 class="pull-right">Trips Summaries</h1>

<h2>Trip Summary</h2>
<br />
<table class="table table-bordered">
<tr>
	<th>Trip ID</th>
	<th>Type</th>
	<th>Total Distance(m)</th>
	<th>Start Time</th>
	<th>End Time</th>
</tr>
<% summary =  Trip.SummaryGet("1335002300","1335016801") %>
<% summary.each do |data| %>
<tr>
	<td><%= data["trip_id"] %></td>
	<td><%= data["type"] %></td>
	<td><%= data["total_m"] %></td>
	<td><%= data["start_timestamp"] %></td>
	<td><%= data["end_timestamp"] %></td>
</tr>

<% end %>
</table>

<br />
<h2>Trip Summary Detail</h2>
<br />

<table class="table table-bordered">
<tr>
	<th>ID</th>
	<th>Trip ID</th>
	<th>Latitude</th>
	<th>Longitude</th>
	<th>Time</th>
</tr>

<% sum_detail = Trip.SummaryDetailGet("1") %>
<% sum_detail.each do |data| %>
<tr>
	<td><%= data["id"] %></td>
	<td><%= data["trip_id"] %></td>
	<td><%= data["lat"] %></td>
	<td><%= data["lon"] %></td>
	<td><%= data["timestamp"] %></td>
</tr>
<% end %>
</table>
